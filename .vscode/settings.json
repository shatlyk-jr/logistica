{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    // Remove .fvm files from the search
    "search.exclude": {
      "**/.fvm": true,
      ".dart_tool": true,
      "build": true,
      "ios/.gems": true
    },
    // Remove from file watching
    "files.watcherExclude": {
      "**/.fvm": true
    }
  }